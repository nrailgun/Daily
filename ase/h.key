\sorts {
	Car;
	Person;
}

\predicates {
	driver(Person, Car);
	adult(Person);
	isRed(Car);
}

\problem {
	(
		(\forall Car c; \exists Person p; driver(p, c))
		&
		(\forall Person p; (\exists Car c; driver(p, c) -> adult(p)))
		&
		(\exists Car c; isRed(c))
	) -> (
		\exists Person p; adult(p)
	)
}
